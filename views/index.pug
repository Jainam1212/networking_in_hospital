        script.
            const sliderInner = document.querySelector(".slider-inner123");
            const prevButton = document.querySelector(".slider-prev-btn");
            const nextButton = document.querySelector(".slider-next-btn");

            let currentSlide1 = 0;

            function goToSlide(slide1) {
              sliderInner.style.transform = `translateX(-${slide1 * 100}%)`;
              currentSlide1 = slide1;
            }

            function nextSlide() {
              if (currentSlide1 === 1) {
                goToSlide(0);
              } else {
                goToSlide(currentSlide1 + 1);
              }
            }

            function prevSlide() {
              if (currentSlide1 === 0) {
                goToSlide(1);
              } else {
                goToSlide(currentSlide1 - 1);
              }
            }

            nextButton.addEventListener("click", nextSlide);
            prevButton.addEventListener("click", prevSlide);


