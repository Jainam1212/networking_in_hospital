  script. 
    const image = document.getElementById('einstien');
    const textbox = document.getElementById('textMsg');
    const box = document.getElementById('chatForm');
    box.style.justifyContent= 'center';
    box.style.flexDirection='column';
    image.addEventListener('mouseenter',()=>{
      textbox.style.display='block';
    })
    image.addEventListener('mouseleave',()=>{
      textbox.style.display='none';
    })
    let visibility = false;
    image.addEventListener('click',(event)=>{
      event.preventDefault();
      visibility = !visibility;
      if (visibility) {
        box.style.display='flex';
      }
      else{
        box.style.display='none';
      }
    })
    function predictData(){
      const history = document.getElementById('chatHistory');
      const info = document.getElementById('symptom').value;      
      const fetchurl = `/chatWithAi?value=${info}`;
      const p = document.createElement('p');
      p.style.float='left';
      p.style.background='white';
      p.style.width='55%';
      p.style.clear='both';
      p.innerText = info;
      history.appendChild(p);
      console.log(fetchurl);
      fetch(fetchurl).then(response => response.text()).then(data=>{
        const str = 'I think you might have '+data;
        const p2 = document.createElement('p');
        p2.innerText = str;
        p2.style.float='right';
        p2.style.background='white';
        p2.style.width='55%';
        p2.style.clear='both';
        history.appendChild(p2);
        console.log(str)});
    }
    const btn = document.getElementById('hide')
    btn.addEventListener('click',(event)=>{
      event.preventDefault();
      box.style.display='none'
    })
    function close(){
      console.log('close');
      box.style.display='none';
    }

